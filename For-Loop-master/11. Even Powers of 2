using System;

namespace Task_11
{
    class EvenPowersOf2
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            for (int i = 1; i <= n; i++)
            {
                if (i == 1)
                {
                    Console.WriteLine(i);
                }
                if (i % 2 == 0)
                {
                    double result = Math.Pow(2, i);
                    Console.WriteLine(result);
                }
                
            }
        }
    }
}
