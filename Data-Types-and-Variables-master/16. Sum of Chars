using System;

namespace Task_16
{
    class SumOfChars
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            int sum = 0;

            for (int i = 0; i < n; i++)
            {
                char ch = Convert.ToChar(Console.ReadLine());
                sum += ch;
            }

            Console.WriteLine("The sum equals: {0}", sum);
        }
    }
}
