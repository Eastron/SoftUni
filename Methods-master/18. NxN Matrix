using System;
using System.Collections.Generic;
using System.Linq;

namespace Task_01
{
    class Program
    {
        public static void PrintMatrix(int num)
        {
            for (int i = 0; i < num; i++)
            {
                for (int j = 0; j < num; j++)
                {
                    Console.Write("{0} ", num);
                }
                Console.WriteLine();
            }
        }

        public static void Main()
        {
            int num = int.Parse(Console.ReadLine());

            PrintMatrix(num);
        }
    }
}
