using System;

namespace Task_06
{
    class FruitOrVegetable
    {
        static void Main(string[] args)
        {
            string some = Console.ReadLine();

            if (some == "banana" || some == "apple" || some == "kiwi" || some == "cherry" || some == "lemon" || some == "grapes")
            {
                Console.WriteLine("fruit");
            }
            else if (some == "tomato" || some == "cucumber" || some == "pepper" || some == "carrot")
            {
                Console.WriteLine("vegetable");
            }
            else
            {
                Console.WriteLine("unknown");
            }
        }
    }
}
