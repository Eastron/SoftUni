using System;

namespace Task_02
{
    class Password
    {
        static void Main(string[] args)
        {
            string account = Console.ReadLine();
            string password = Console.ReadLine();

            string input = Console.ReadLine();
            while (input != password)
            {
                input = Console.ReadLine();
            }

            Console.WriteLine("Welcome {0}!", account);
        }
    }
}
