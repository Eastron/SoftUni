using System;

namespace Task_03
{
    class Sequence
    {
        static void Main(string[] args)
        {
            int number = int.Parse(Console.ReadLine());
            int count = 1;

            while (count <= number)
            {
                Console.WriteLine(count);
                count = count * 2 + 1;
            }
           
        }
    }
}
