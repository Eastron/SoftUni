using System;
		
public class Program
{
	public static void Main()
	{
		int n = int.Parse(Console.ReadLine());
		
		int lenght = n * 2;
		int sum = 0;
		
		for(int i = 0; i < lenght; i++)
		{
			if(i % 2 != 0)
			{
				sum += i;
				Console.WriteLine(i);
			}
		}
		Console.WriteLine("Sum: {0}",sum);
	}
}
